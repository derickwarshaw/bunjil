        "graphql-yoga": "1.2.0",
        "jsonwebtoken": "8.1.1",
        "koa": "^2.4.1",
        "koa-bodyparser": "^4.2.0",
        "koa-compose": "^4.0.0",
        "koa-compress": "^2.0.0",
